//ARRAYS CODE 2D 
#include<stdio.h> 
int main(){ 
int marks[2][3]; 
marks[0][0]=90; 
marks[0][1]=99; 
marks[0][2]=98; 
marks[1][0]=90; 
marks[1][1]=99; 
marks[1][2]=98; 
printf("chemistry marks for 1 are\t 
%d\n\n",marks[0][0]); 
printf("bio marks for 1 are\t %d\n",marks[0][1]); 
printf("CS marks for 1 are\t %d\n",marks[0][2]); 
return 0; 
} 
