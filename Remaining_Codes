void hi(); 
void bye(); 
int main() 
{ 
printf("enter i if you want me & g if you dont"); 
char ch; 
scanf("%d",&ch); 
if(ch== 'i'){ 
hi(); 
} 
else{ 
bye(); 
} 
} 
void hi(){ 
printf("hi"); 
} 
void bye(){ 
printf("bye"); 
} 
