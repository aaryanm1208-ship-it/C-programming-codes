//strings code b,a = o/p= aaryan 
//* then a,b =o/p= mudvikar 
#include<stdio.h> 
#include<string.h> 
int main(){ 
char a[]="aaryan"; 
char b[]="mudvikar"; 
strcpy(b,a); 
puts(a); 
return 0; 
} 
