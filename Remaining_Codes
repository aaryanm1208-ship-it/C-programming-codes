
//ARRAYS CONCEPT  
#include<stdio.h> 
int main(){ 
int marks[3]; 
printf("chemistry mein muze mile hai\n"); 
scanf("%d",&marks[0]); 
printf("maths mein muze mile hai\n"); 
scanf("%d",&marks[1]); 
printf("IT mein muze mile hai\n"); 
scanf("%d",&marks[2]); 
printf("chemistry=%d\n,maths=%d\n,IT=%d\n",marks[
0],marks[1],marks[2]); 
return 0; 
} 
