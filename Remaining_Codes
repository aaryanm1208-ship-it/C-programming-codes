void square(int n); 
int main(){ 
int number=4; 
square(number); 
printf("square of number %d\n",number); 
return 0; 
} 
void square(int n){ 
n =n * n; 
printf("%d\n",n); 
} 
45

//pointer to function call 
#include <stdio.h> 
void square(int n); 
void _square(int *n); 
int main(){ 
int number=4; 
square(number); 
printf("square of number %d\n",number); 
_square(&number); 
printf("soul %d",number); 
return 0; 
} 
void square(int n){ 
n =n * n; 
printf("%d\n",n); 
} 
void _square(int *n){ 
*n =(*n) * (*n); 
printf("%d\n",*n); 
} 
46

//swap 2 numbers a and b 
#include<stdio.h> 
void num(int a , int b); 
void _num(int *a , int *b); 
int main(){ 
int x=2 ,  y=4; 
_num(&x , &y); 
printf("%d\n%d\n",x,y); 
return 0; 
} 
void _num(int *a , int *b){ 
int t=*a; 
*a=*b; 
*b=t; 
printf("*a=%d\n , *b=%d\n", *a,*b); 
} 
void num(int a , int b){ 
int t=a; 
a=b; 
b=t; 
printf("a=%d\n , b=%d\n", a,b); 
} 
