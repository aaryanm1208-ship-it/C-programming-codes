//ARRAYS USING POINTER ARITHMETIC 
#include<stdio.h> 
int main(){ 
int a=2; 
int *ptr=&a; 
printf("value of ptr%u\n",ptr); 
ptr++; 
printf("value of ptr%u\n",ptr); 
return 0; 
} 
51

//pointer arithmetic using both increment and 
decrement  
#include<stdio.h> 
int main(){ 
int a=2; 
int *ptr=&a; 
printf("value of ptr is%d\n",ptr); 
ptr++; 
printf("value of ptr is%d\n",ptr); 
ptr--; 
printf("value of ptr is%d\n",ptr); 
return 0; 
} 
