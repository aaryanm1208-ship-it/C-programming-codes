
//structures code  
#include<stdio.h> 
#include<string.h> 
struct student { 
int clas; 
float cgpa; 
char name[]; 
}; 
int main(){ 
struct student s1; 
s1.clas=11; 
s1.cgpa=9.99; 
strcat(s1.name,"AARYAN"); 
printf("my name is:%s\n","AARYAN"); 
printf("%d\n",s1.clas); 
printf("%f\n",s1.cgpa); 
return 0; 
}
