
#include<stdio.h> 
int main(){ 
int a=1; 
int _a=1; 
int *ptr=&a; 
int *_ptr=&_a; 
printf("differnce = \t%u\n",ptr-_ptr); 
printf("comparison = \t%u\n",ptr == _ptr); 
return 0; 
} 
