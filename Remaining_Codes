
//TO FIND THE FACTORIAL OF A NUMBER  
#include<stdio.h> 
int main() 
{ 
int n; 
printf("enter the value"); 
scanf("%d",&n); 
int fact=1; 
for(int i=1 ; i<=n ; i++){ 
fact=fact*i; 
} 
printf("the factorial = %d\n",fact); 
printf("THANK YOU"); 
return 0; 
} 
