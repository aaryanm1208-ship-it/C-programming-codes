//TO SKIP THE NUMBER YOU DONT WANT 
#include<stdio.h> 
int main() 
{ 
for(int n=1 ; n<=18 ; n++){ 
if(n==7){ 
continue; 
} 
printf("%d\n",n); 
} 
printf("THANK YOU"); 
return 0; 
} 
