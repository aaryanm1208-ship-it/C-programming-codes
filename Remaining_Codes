// TABLE OF THE NUMBERS 
#include<stdio.h> 
int main() 
{ 
int n; 
printf("KYA DEKH RAHE HO\n,DUM HAI TO VALUE 
ENTER KARKE DIKHAO\n"); 
scanf("%d",&n); 
for(int i=1 ; i<=20 ; i++){ 
printf("%d\n",i*n); 
} 
return 0; 
} 
32

// USING BREAK TO END THE LOOP 
#include <stdio.h> 
int main() { 
for(int i=1 ; i<=15 ; i++){ 
if(i==3){ 
break;} 
printf("%d\n",i); 
} 
printf("end\n"); 
return 0; 
} 
