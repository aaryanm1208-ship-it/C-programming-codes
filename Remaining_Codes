#include<stdio.h> 
int sum(int n); 
int main(){ 
printf("sum is %d\n",sum(17)); 
return 0; 
} 
int sum(int n){ 
if(n==2){ 
return 1; 
} 
int sumNm1=sum(n-1); 
int sumN= sumNm1 + n; 
return sumN; 
} 
