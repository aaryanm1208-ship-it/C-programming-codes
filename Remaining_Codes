
// RO KEEP TAKING THE NUMBERS FORM USER UNTIL 
ITS AN ODD NUMBER 
#include <stdio.h> 
int main() { 
int n; 
do{ 
printf("enter the number :\n"); 
scanf("%d",&n); 
printf("%d\n",n); 
if(n % 2 != 0){ 
break; 
} 
}while(1); 
printf("thankyou"); 
return 0; 
} 
